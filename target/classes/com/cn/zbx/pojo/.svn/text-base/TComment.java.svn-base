package com.cn.zbx.pojo;

import java.util.Date;

public class TComment {
    private Long commId;

    private String commContent;

    private Long resId;

    private Long userId;

    private Integer commIsShow;

    private Integer commIsDelete;

    private Long createId;

    private Date commCreateTime;

    private Date commUpdateTime;

    public Long getCommId() {
        return commId;
    }

    public void setCommId(Long commId) {
        this.commId = commId;
    }

    public String getCommContent() {
        return commContent;
    }

    public void setCommContent(String commContent) {
        this.commContent = commContent == null ? null : commContent.trim();
    }

    public Long getResId() {
        return resId;
    }

    public void setResId(Long resId) {
        this.resId = resId;
    }

    public Long getUserId() {
        return userId;
    }

    public void setUserId(Long userId) {
        this.userId = userId;
    }

    public Integer getCommIsShow() {
        return commIsShow;
    }

    public void setCommIsShow(Integer commIsShow) {
        this.commIsShow = commIsShow;
    }

    public Integer getCommIsDelete() {
        return commIsDelete;
    }

    public void setCommIsDelete(Integer commIsDelete) {
        this.commIsDelete = commIsDelete;
    }

    public Long getCreateId() {
        return createId;
    }

    public void setCreateId(Long createId) {
        this.createId = createId;
    }

    public Date getCommCreateTime() {
        return commCreateTime;
    }

    public void setCommCreateTime(Date commCreateTime) {
        this.commCreateTime = commCreateTime;
    }

    public Date getCommUpdateTime() {
        return commUpdateTime;
    }

    public void setCommUpdateTime(Date commUpdateTime) {
        this.commUpdateTime = commUpdateTime;
    }
}