package com.cn.zbx.util;

import com.cn.zbx.pojo.TUser;

public class Util {
	
	/**
	 * 根据返回数量判断ture / false
	 * @param num
	 * @return
	 */
	public static boolean isSuccessByNum(Integer num){
		if(num > 0){
			return true;
		}else{
			return false;
		}
	}
	
	public static String getTokenByPhone(TUser user){
		if(user == null){
			return null;
		}else{
			String userPhone = user.getUserPhone();
			if(userPhone == null){
				return null;
			}else{
				return MD5Util.MD5Encode(userPhone);
			}
		}
	}
	
	
}
